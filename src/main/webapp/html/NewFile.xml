<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="60"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="60"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="1024"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="11"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="5"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="SMALLINT" hibernate-type="java.lang.Short"
			not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-catalog="law" match-name="t_user" />
	<table catalog="law" name="t_user">
		<column name="account"></column>
		<column name="phone"></column>
		<column name="concerns"></column>
		<column name="category"></column>
		<column name="password"></column>
		<column name="contributions"></column>
	</table>
</hibernate-reverse-engineering>